  <div class="alert alert-success alert-dismissible" role="alert" id="alert">
    <%= t(notice) %>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true" onclick="closeAlert()">&times;</span>
